namespace Intrusion.Server
{
	public class SideTechnology
	{
		// Creates a SideTechnology object.
		public constructor("_side" as Side, "_technology" as Technology) {
			_self.Side = _side;
			_self.Technology = _technology;
		};
		
		public property Side Side { get; private set; };
		
		public property Technology Technology { get; private set; };
	};
};
